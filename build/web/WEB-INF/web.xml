        <servlet-name>msg_submit</servlet-name>
        <url-pattern>/msg_submit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadBid</servlet-name>
        <url-pattern>/UploadBid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_review</servlet-name>
        <url-pattern>/user_review</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_verify</servlet-name>
        <url-pattern>/user_verify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageRetrieve</servlet-name>
        <url-pattern>/ImageRetrieve</url-pattern>
    </servlet-mapping>
