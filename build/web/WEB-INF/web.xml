        <servlet-class>user_review</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_verify</servlet-name>
        <servlet-class>user_verify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageRetrieve</servlet-name>
        <servlet-class>ImageRetrieve</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateBid</servlet-name>
        <url-pattern>/CreateBid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBid</servlet-name>
        <url-pattern>/EditBid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>msg_delete</servlet-name>
        <url-pattern>/msg_delete</url-pattern>
    </servlet-mapping>
